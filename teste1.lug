main{
    var int i;
    var bool flag;
    i := 0;
    flag := Menor (i, 10);

    while ((flag == true)) do {
        print (i);
        i := (i + 1);
        flag := Menor (i, 10);
    };

}

function bool Menor (int x, int y){
    if ((x < y)){
        return true;
    };
    return false;
}